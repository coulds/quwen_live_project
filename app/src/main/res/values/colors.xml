<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- App 样式中引用的颜色 -->
    <color name="colorPrimary">@color/white</color>
    <color name="colorPrimaryDark">@color/black</color>
    <color name="colorAccent">#69E8FE</color>
    <color name="windowBackground">#F4F4F4</color>
    <color name="textColor">#333333</color>
    <color name="textColorHint">#757575</color>

    <!-- 按钮按压时的颜色 -->
    <color name="colorButtonPressed">#AA69E8FE</color>
    <!-- 按钮禁用时的颜色 -->
    <color name="colorButtonDisable">#BBBBBB</color>
    <!-- 分割线的颜色 -->
    <color name="colorLine">#CCCCCC</color>
    <!-- 图标颜色 -->
    <color name="colorIcon">#7C7C7C</color>

    <!-- 对话框确认按钮文本颜色 -->
    <color name="colorConfirm">#007AFF</color>
    <!-- 对话框取消按钮文本颜色 -->
    <color name="colorCancel">#F44336</color>
    <!--  注册提示文本颜色  -->
    <color name="color999">#999999</color>

</resources>