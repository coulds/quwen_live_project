                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    app:layout_constraintTop_toBottomOf="@+id/touxiang">

                    <LinearLayout
                        android:id="@+id/gz_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/touxiang">

                        <TextView
                            android:id="@+id/gz_number"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="567"
                            android:textSize="22sp" />

                        <TextView
                            android:id="@+id/gz_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="关注"
                            android:textColor="#b7b7b7" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/fs_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/touxiang">

                        <TextView
                            android:id="@+id/fs_number"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="567"
                            android:textSize="22sp" />

                        <TextView
                            android:id="@+id/fs_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="粉丝"
                            android:textColor="#b7b7b7" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/jyz_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical"
